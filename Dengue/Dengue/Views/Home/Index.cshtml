@model IEnumerable<Dengue.Models.DengueCluster>
@{
    ViewBag.Title = "Home";
}
@using Jmelosegui.Mvc.GoogleMap
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
<script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyDY0kkJiTPVd2U7aTOAwhc9ySH6oHxOIYM&sensor=false"></script>
<script src="~/Scripts/DengueJS.js"></script>

<style>
    #toStyleDiv a {
        color: white;
    }
</style>

<div style="padding-right:0px; padding-bottom:2px;">
    <div id="googleMap" style="height:330px;"></div>
</div>
<div style="width:50%; float:left; height:311px; border-radius:8px; border: 1px solid #a09e9e;">
    <div style="background-color: rgb(245,248,250); height:35px; text-align:center; padding-top:8px; color: #282020;">
        Why should you prevent Dengue?:
    </div>
    <iframe style="border:8px solid #000; border-radius: 8px" width="429" height="274" src="https://www.youtube.com/embed/Sr6x76ME6QA" frameborder="0" allowfullscreen></iframe>
</div>
<div style="width:50%; float:right; height:311px; ">
    <div style="margin-left:2px; border-radius:8px; border: 1px solid #a09e9e; height:100%;">
        <div style="background-color: rgb(245,248,250); height:35px; text-align:center; padding-top:8px; color: #282020;">
            Here are some statistics:
            <img style="margin-top:10px;" src="@Url.Action("DrawLineChart", "Home", new { height = 255})" />
        </div>
    </div>
</div>

@section scripts
{
    @(Html.GoogleMap().ScriptRegistrar())
}